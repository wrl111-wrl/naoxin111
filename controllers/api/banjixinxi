import request from '@/utils/request';
// list
export const qingjiashenqingList = (params) => {
    return request({
        url: '/qingjiashenqing/page',
        method: 'post',
        data: params,
    })
};
export const qingjiashenqingDetail = (id) => {
    return request({
        url: '/qingjiashenqing/'+id,
        method: 'get',
    })
};
// add
export const qingjiashenqingSave = (params) => {
    return request({
        url: '/qingjiashenqing',
        method: 'post',
        data: params,
    })
};
// updt
export const qingjiashenqingEdit = (params) => {
    return request({
        url: '/qingjiashenqing',
        method: 'put',
        data: params,
    })
};
// delete
export const qingjiashenqingDelete = (id) => {
    return request({
        url: '/qingjiashenqing/'+id,
        method: 'delete',
    })
};
// pldel
export const qingjiashenqingDeleteList = (qingjiashenqings) => {
    return request({
        url: '/qingjiashenqing/deleteList',
        data:{list:qingjiashenqings},
        method: 'post',
    })
};
// all
export const qingjiashenqingAllList = () => {
    return request({
        url: '/qingjiashenqing',
        method: 'get',
    })
};

